{
    "name": "barth/site-src",
    "description": "Source for building my personal website",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "doctrine/cache": "^2.0",
        "intervention/image": "^2.7",
        "knplabs/github-api": "^3.0",
        "kornrunner/blurhash": "^1.2",
        "league/commonmark": "^2.0",
        "mnapoli/front-yaml": "^2.0",
        "nesbot/carbon": "^2.53",
        "nyholm/psr7": "^1.2",
        "phpdocumentor/reflection-docblock": "^5.2",
        "ramsey/collection": "^1.0",
        "symfony/asset": "^6.0",
        "symfony/browser-kit": "^6.0",
        "symfony/cache": "^6.0",
        "symfony/console": "5.4.*|^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/dom-crawler": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/flex": "^2.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-client-contracts": "^2.0|^3.0",
        "symfony/http-foundation": "^6.0",
        "symfony/mime": "^6.0",
        "symfony/property-access": "^6.0",
        "symfony/property-info": "^6.0",
        "symfony/runtime": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/string": "^6.0",
        "symfony/twig-bridge": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/ux-lazy-image": "^2.0",
        "symfony/ux-turbo": "^2.0",
        "symfony/validator": "^6.0",
        "symfony/webpack-encore-bundle": "^1.11",
        "symfony/yaml": "^5.4|^6.0",
        "symfonycorp/connect": "^8.0",
        "symplify/symfony-static-dumper": "^9.0",
        "thecodingmachine/safe": "^2.0",
        "twig/extra-bundle": "^3.0",
        "twig/intl-extra": "^3.0",
        "twig/markdown-extra": "^3.0"
    },
    "require-dev": {
        "ergebnis/phpstan-rules": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.2",
        "nunomaduro/phpinsights": "^2.4",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpmd/phpmd": "^2.10",
        "phpro/grumphp": "^1.5",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "sebastian/phpcpd": "^6.0",
        "seld/jsonlint": "^1.8",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/maker-bundle": "^1.14",
        "symfony/stopwatch": "^6.0",
        "symfony/web-profiler-bundle": "^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*|^6.0"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "stable"
}
